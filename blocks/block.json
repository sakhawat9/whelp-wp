{
	"apiVersion": 2,
	"name": "create-block/whatsapp-button",
	"title": "WhatsApp Button",
	"category": "whatsapp-block",
	"icon": "whatsapp",
	"description": "A block for whatsapp to show interactive whatapp button with nice way.",
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"text": true,
			"gradients": true,
			"hover": true
		},
		"spacing": {},
		"typography": {}
	},

	"example": {
		"attributes": {
			"text": "This is some text!",
			"gradient": "red-to-blue",
			"links": true
		}
	},
	"textdomain": "ta-whatshelp",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"attributes": {
		"text": {
			"type": "string",
			"source": "html",
			"selector": "button",
			"default": "How can I help you?"
		},
		"info": {
			"type": "string",
			"source": "html",
			"selector": "p",
			"default": "Robert / Sales Support"
		},
		"title": {
			"type": "string",
			"source": "html",
			"selector": "p",
			"default": "How can I help you?"
		},
		"online": {
			"type": "string",
			"source": "html",
			"selector": "p",
			"default": "I am online"
		},
		"buttonType": {
			"type": "string",
			"default": "basic-button"
		},
		"buttonSize": {
			"type": "string",
			"default": "size-normal"
		},
		"borderRadius": {
			"type": "string",
			"default": "border-radius-rounded"
		},
		"visibility": {
			"type": "string",
			"default": ""
		},
		"border": {
			"type": "string",
			"default": ""
		},
		"textAlignment": {
			"type": "string",
			"default": "left"
		},
		"buttonUrl": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "href"
		},
		"links": {
			"type": "boolean",
			"default": false
		},
		"iconTarget": {
            "type": "boolean",
            "default": false
        },
		"buttonLinkTarget": {
            "type": "boolean",
            "default": false
        },
		"buttonTextColor": {
			"type": "string"
		},
		"buttonBackgroundColor": {
			"type": "string",
			"default": "red"
		}
	}
}
